<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/day_event">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:background="@color/white">

        <TextView
            android:id="@+id/text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/myLinearLayout"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="15dp"
            android:text=""
            android:textColor="@android:color/black"
            android:textSize="12sp" />


        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="18dp"
            android:layout_marginBottom="8dp"
            app:srcCompat="@drawable/notified" />


        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="1px"
            android:layout_height="match_parent"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="67dp"
            android:background="#e5e5e5" />

        <!--<ImageView-->
        <!--android:id="@+id/imageView5"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:layout_alignParentStart="true"-->
        <!--android:layout_centerVertical="true"-->
        <!--android:layout_marginStart="69dp"-->
        <!--app:srcCompat="@drawable/ic_lens_black_24dp" />-->

        <com.honhai.foxconn.fxccalendar.colormanager.ColorImageView
            android:id="@+id/imageView5"
            android:layout_width="15dp"
            android:layout_height="44dp"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="68dp" />

        <!--<View-->
            <!--android:layout_width="255dp"-->
            <!--android:layout_height="1dp"-->
            <!--android:layout_centerVertical="true"-->
            <!--android:layout_marginLeft="30dp"-->
            <!--android:background="@android:color/black" />-->

        <LinearLayout
            android:id="@+id/myLinearLayout"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_marginStart="88dp"
            android:layout_marginBottom="0dp"
            android:gravity="center_vertical">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentBottom="true"
                android:ellipsize="end"
                android:singleLine="true"
                android:maxEms="11"
                android:text="今天放假"
                android:textColor="@android:color/black" />

            <ImageView
                android:id="@+id/alarm"
                android:layout_width="11dp"
                android:layout_height="11dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/notified"
                android:visibility="gone"/>
        </LinearLayout>

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView6"
            android:layout_alignParentTop="true"
            android:layout_marginStart="-1dp"
            android:layout_marginTop="0dp"
            android:paddingTop="5dp"
            android:text="03：30"
            android:textColor="@android:color/black"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="30dp"
            android:layout_marginBottom="0dp"
            android:paddingBottom="5dp"
            android:text="04：30"
            android:textColor="@android:color/darker_gray"
            android:textSize="12.5sp" />
    </RelativeLayout>
</RelativeLayout>