package com.honhai.foxconn.fxccalendar.colormanager;

import android.widget.ImageView;

public class ColorItem {
    private String colorName;
    private int colorImageId;
    private int clearImg;

    public ColorItem(String colorName, int colorImageId){
        this.colorName = colorName;
        this.colorImageId = colorImageId;
    }

    public ColorItem(String colorName, int colorImageId,int clearImg ){
        this.colorName = colorName;
        this.colorImageId = colorImageId;
        this.clearImg = clearImg;

    }

    public String getColorName(){
        return colorName;
    }


    public int getColorImageId() {
        return colorImageId;
    }

    public int getClearImg() {
        return clearImg;
    }

    public void setClearImg(int clearImg) {
        this.clearImg = clearImg;
    }
}
